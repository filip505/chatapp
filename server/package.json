{
  "name": "mynode",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "NODE_ENV=dev DB_USER=tet nodemon ./bin/dev",
    "build": "npm run clean && mkdir dist && babel src/main -s -d dist",
    "env": "sed 's/$DB_HOST/'$DB_HOST'/g;s/$DB_USER/'$DB_USER'/g;s/$DB_PASSWORD/'$DB_PASSWORD'/g' env.txt > ./src/main/env.js",
    "production": "npm run env && npm run build && NODE_ENV=production node bin/pro",
    "test": "jest --runInBand --coverage"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "http-status-codes": "^1.3.0",
    "onesignal-node": "^2.0.1",
    "pg": "^7.8.0",
    "reflect-metadata": "^0.1.13",
    "tv4": "^1.3.0",
    "typeorm": "^0.2.11",
    "uuid": "^3.3.2",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/cli": "^7.2.3",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "jest": "^24.0.0",
    "nodemon": "^1.18.10"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  },
  "jest": {
    "rootDir": "./src/"
  }
}